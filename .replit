modules = ["python-3.11"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start Muzaquest Analytics"

[workflows.workflow.tasks.preview]
port = 8000

[deployment]
run = ["sh", "-c", "python main.py --help"]
deploymentTarget = "cloudrun"

[env]
PYTHONPATH = "$PYTHONPATH:."